<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

	<grid model="com.axelor.gst.db.City"
		title="City" name="grid-city">
		<field name="name" />
		<field name="state"/>
		<field name="country" />
	</grid>

	<form model="com.axelor.gst.db.City"
		title="City" name="form-city">
		<panel title="Overview" stacked="true">
		<field name="name" />
		<field name="state"/>
		<field name="country" />
		</panel>
		</form>

</object-views>