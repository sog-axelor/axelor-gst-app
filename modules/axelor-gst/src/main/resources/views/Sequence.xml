<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">


	<grid model="com.axelor.gst.db.Sequence" title="Sequence"
		name="grid-sequence">
		<field name="prefix" />
		<field name="suffix" />
		<field name="nextNumber" />
		<field name="padding" />
		<field name="model" />
	</grid>

	<form model="com.axelor.gst.db.Sequence" title="Sequence"
		name="form-sequence">
		<panel title="overview">
			<field name="prefix" onChange="act.sequence.generator"/>
			<field name="suffix" onChange="act.sequence.generator"/>
			<field name="nextNumber"  />
			<field name="padding" onChange="act.sequence.generator"/>
			<field name="model" />
		</panel>
	</form>

	


	<action-method name="act.sequence.generator">
		<call class="com.axelor.gst.web.GstController"
			method="sequence" />
	</action-method>
</object-views>




