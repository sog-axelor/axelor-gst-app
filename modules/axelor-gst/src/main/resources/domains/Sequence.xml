<?xml version="1.0" encoding="UTF-8"?>
<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

	<module name="sequence" package="com.axelor.gst.db" />

	<entity name="Sequence" lang="java">
		<string name="prefix" title="Prefix" required="true" />
		<string name="suffix" title="Suffix" />
		<string name="nextNumber" title="Next Number" />
		<integer name="padding" title="Padding" min="1" max="10"/>
		<many-to-one ref="com.axelor.meta.db.MetaModel" name="model" required="true"/>
	</entity>

</domain-models>

	